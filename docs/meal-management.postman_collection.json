			"name": "4. Order Management",
			"item": [
				{
					"name": "4.1 Create Order (Employee)",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 201\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"var jsonData = pm.response.json();",
									"pm.collectionVariables.set(\"order_id\", jsonData.data.id);",
									"",
									"pm.test(\"Order total calculated correctly\", function () {",
									"    pm.expect(jsonData.data.totalPrice).to.eql(30.00); // 15.00 * 2",
									"});",
									"",
									"pm.test(\"Order status is PENDING\", function () {",
									"    pm.expect(jsonData.data.status).to.eql('PENDING');",
									"});"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Calculate tomorrow's date",
									"var tomorrow = new Date();",
									"tomorrow.setDate(tomorrow.getDate() + 1);",
									"var orderDate = tomorrow.toISOString().split('T')[0];",
									"pm.collectionVariables.set(\"order_date\", orderDate);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": [
								{
									"key": "token",
									"value": "{{employee_token}}",
									"type": "string"
								}
							]
						},
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"employeeId\": {{employee_id}},\n  \"mealId\": {{meal_id}},\n  \"orderDate\": \"{{order_date}}\",\n  \"quantity\": 2\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/v1/orders",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"v1",
								"orders"
							]
						},
						"description": "Employee creates a meal order for a future date"
					},
					"response": []
				},
